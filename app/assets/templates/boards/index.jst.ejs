<% boardHeight = window.innerHeight * 0.33 %>

<div class='boards-index-title'>
  <h3>My Boards</h3>
</div>
<ul class='boards'>
  <% boards.each(function (board) { %>
    <li>
      <a href="#/boards/<%= board.id %>" style='height: <%= boardHeight %>px'>
        <span class='shadow'></span>
        <div class='details' style='line-height: <%= boardHeight - 20%>px;'>
          <span><%= board.escape("title") %></span>
        </div>
      </a>
    </li>
  <% }); %>
</ul>
